    {
    image
    {
      	function			= FLAT_BOX
        detail				= "tooltip"
        recolorable			= TRUE
        file				= "Others/tooltip-bg.png"
        border			= { 12, 12, 8, 8 }
        stretch			= TRUE
    }
  }
}

