      	overlay_file			= "Range/slider-vert.png"
      	overlay_stretch		= FALSE
      	orientation			= VERTICAL
    }
    image 
    {
      	function			= SLIDER
      	recolorable			= TRUE
     	state           		= PRELIGHT
      	file				= "Range/null.png"
      	border			= { 0, 0, 0, 0 }
      	stretch			= TRUE
      	overlay_file			= "Range/slider-vert-prelight.png"
      	overlay_stretch		= FALSE
     	 orientation		= VERTICAL
    }
    image 
    {
      	function			= SLIDER
      	recolorable			= TRUE
      	state           		= INSENSITIVE
      	file				= "Range/null.png"
      	border			= { 0, 0, 0, 0 }
      	stretch			= TRUE
      	overlay_file			= "Range/slider-vert.png"
      	overlay_stretch		= FALSE
      	orientation			= VERTICAL
    }
  }
}

